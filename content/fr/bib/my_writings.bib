@inproceedings{cellierThermalFallingFilm2017,
  title = {Thermal Falling Film Modelling, Validation and Exploration},
  booktitle = {23\`eme Congr\`es Fran\c{c}ais de {{M\'ecanique}}},
  author = {Cellier, Nicolas},
  year = {2017},
  month = sep,
  address = {{Lille}},
  abstract = {Les transferts de chaleur \`a travers un film ruisselant sur une plaque chauff\'e d\'epend non seulement des caract\'eristiques thermiques du fluid mais aussi des caract\'eristiques de l'\'ecoulement: les travaux exp\'erimentaux montrent que la pr\'esence d'instabilit\'es hydrodynamiques peuvent mener \`a des intensifications de transfert non n\'egligeable compar\'es \`a un film ruisselant plat. Afin de reproduire ces r\'esultats, un mod\`ele asymptotique bas\'e sur une m\'ethode aux r\'esidus pond\'er\'es int\'egr\'es (mod\`ele dit WRIBL) a \'et\'e d\'evelopp\'e \`a la fois pour l'hydrodynamique (bas\'e sur les \'equations de Navier et Stokes) et les transferts de chaleur (bas\'es sur l'\'equation de Fourrier). Bien moins couteux que les m\'ethodes num\'eriques directes, la r\'esolution num\'erique de ce mod\`ele permet une exploration rapide des diff\'erents param\`etres physiques afin de mieux comprendre l'interaction hydrodynamique/thermique. L'objectif \`a terme est d'utiliser ces outils et la compr\'ehension de ces interactions pour am\'eliorer les performances des \'equipements industriels tirant partis de ces ph\'enom\`enes, en particulier de dimensionner la forme des plaques des \'echangeurs afin de forcer l'intensification. Dans ce but, les mod\`eles ont \'et\'e \'etendus afin de travailler sur un support d'\'ecoulement corrugu\'e plut\^ot que plat. Une premi\`ere partie de l'\'etude a pour objectif de v\'erifier le domaine d'applicabilit\'e du mod\`ele, et une comparaison sera faite entre notre mod\`ele asymptotique et la r\'esolution directe de l'\'equation de Fourrier Une seconde partie concerne la reproductibilit\'e des r\'esultats observ\'es exp\'erimentalement sur l'intensification des transferts thermiques le long d'un film ruisselant chauff\'e. Enfin, une troisi\`eme partie concerne l'interaction entre des r\'egimes hydrodynamiques s\'electionn\'e par un for\c{c}age en fr\'equence et le transfert de chaleur \`a travers le film.},
  file = {/home/nicolas/Zotero/storage/Y2NHBS5F/Cellier et al. - 2017 - Thermal falling film modelling validation and exploration.pdf},
  keywords = {⛔ No DOI found,falling films,free sur-,heat exchangers,transfer intensification}
}

